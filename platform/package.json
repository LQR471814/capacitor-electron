    "@types/node": "~14.17.4",
    "conventional-changelog-cli": "~2.1.1",
    "conventional-changelog-conventionalcommits": "~4.6.0",
    "electron": "~13.1.6",
    "esbuild": "~0.12.15",
    "eslint": "~7.27.0",
    "np": "~7.5.0",
    "prettier": "2.3.2",
    "rimraf": "~3.0.2",
    "tar": "^6.1.7",
    "tslib": "~2.3.0",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "@capacitor/cli": "^3.1.1",
    "@capacitor/core": "^3.1.1",
    "@ionic/utils-fs": "~3.1.5",
    "chalk": "~4.1.1",
    "electron-is-dev": "~2.0.0",
    "events": "~3.3.0",
    "fs-extra": "~9.1.0",
    "mime-types": "~2.1.31",
    "ora": "~5.4.1"
  },
  "repository": {
    "type": "git",
